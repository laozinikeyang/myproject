<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="context1">
		<jdbcConnection
			connectionURL="jdbc:mysql://localhost:3306/myproject"
			driverClass="com.mysql.jdbc.Driver" password="root" userId="root" />
		<javaModelGenerator targetPackage="sqlmapping"
			targetProject="myproject" />
		<sqlMapGenerator targetPackage="sqlmapping"
			targetProject="myproject" />
		<javaClientGenerator targetPackage="sqlmapping"
			targetProject="myproject" type="XMLMAPPER" />
		<table tableName="%" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false">
		</table>
